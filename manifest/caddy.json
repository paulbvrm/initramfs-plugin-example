{
  "$schema": "https://raw.githubusercontent.com/volantvm/volant/main/docs/schemas/plugin-manifest-v1.json",
  "schema_version": "1.0",
  "name": "caddy",
  "version": "0.1.0",
  "runtime": "caddy",
  "enabled": true,
  "initramfs": {
    "url": "https://github.com/volantvm/initramfs-plugin-example/releases/download/v0.1.0/plugin.cpio.gz",
    "checksum": "deba6998b5852a4fbdc07f47b52be03fe1479233f9f12775d6c9a14705139ba7"
  },
  "resources": {
    "cpu_cores": 1,
    "memory_mb": 512
  },
  "workload": {
    "type": "http",
    "entrypoint": [
      "/usr/bin/caddy",
      "run",
      "--config",
      "/etc/caddy/Caddyfile"
    ],
    "base_url": "http://127.0.0.1:80",
    "workdir": "/",
    "env": {
      "PATH": "/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
    }
  },
  "health_check": {
    "endpoint": "/",
    "timeout_ms": 10000
  }
}
